              <div class="mt-5">
                <%= button_to "Contact Us", contact_path, method: :get, data: {turbo: false}, class: "btn-primary"%>
              </div>
            </div>
          </div>
        </div>
        <div class="pt-6">
          <div class="flow-root rounded-lg bg-gray-50 px-6 pb-8">
